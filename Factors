
/**
 * A program that gives you the factors for any positive number
 * Let a factor of n be defined as a number f such that n % f == 0
 * @LMAD
 * @4/4
 */
import java.util.Scanner;
public class Factors
{
    public static void main(String[] args)
    {
        Scanner in = new Scanner(System.in);
        
        System.out.println("Please enter a positive number");
        int n =in.nextInt();
        
        for(int i = 1; i<=n/2; i++)
        {
            if(n%i == 0)
            {
                System.out.print(i + " ");
            }
        }
        System.out.println("and " + n);
    }
}
      
       
    

